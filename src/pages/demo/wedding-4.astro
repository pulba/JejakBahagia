        <div class=`flex flex-col sm:flex-row justify-center items-center gap-11 gap-y-11 mx-auto *:w-72 *:max-w-full *:h-full *:rounded-lg *:py-8 *:px-4 *:relative *:${frontmatter.bgfs}`>
          <div class="fade-right opacity-0">
            <div
              class="rounded-lg w-full h-full aspect-square bg-cover bg-center bg-white flex justify-center items-center"
              style={`background-image:url(${frontmatter.pm1})`}
            ></div>
            <h3 class={`fade-down opacity-0 text-left text-8xl mt-6 font-nautigal ${frontmatter.txsc}`}>{frontmatter.female}</h3>
            <div class="fade-up opacity-0 font-poppins space-y-3">
              <div class={`${frontmatter.txth}`}>
                <p class={`text-xl font-bold border-b-2 ${frontmatter.bdr}`}>{frontmatter.fullfemale}</p>
                <p>{frontmatter.childfemale}</p>
                <p class={`text-lg font-bold ${frontmatter.txsc}`}>{frontmatter.parentfemale}</p>
                <p>{frontmatter.cityfemale}</p>
                <div class="mt-4">
                  <a href={`https://www.instagram.com/${frontmatter.instagramfemale}`} class="flex flex-row gap-2 items-center">
                    <Icon name="mdi:instagram" class={`w-10 h-10 p-2 rounded-full ${frontmatter.txfs} ${frontmatter.bgsc}`} />
                  </a>
                </div>
              </div>
            </div>
          </div>
          <div class="fade-left opacity-0">
            <div
              class="rounded-lg w-full h-full aspect-square bg-cover bg-center bg-white flex justify-center items-center"
              style={`background-image:url(${frontmatter.pm2})`}
            ></div>
            <h3 class={`fade-down opacity-0 text-left text-8xl mt-6 font-nautigal ${frontmatter.txsc}`}>{frontmatter.male}</h3>
            <div class="fade-up opacity-0 font-poppins space-y-3">
              <div class={`${frontmatter.txth}`}>
                <p class={`text-xl font-bold border-b-2 ${frontmatter.bdr}`}>{frontmatter.fullmale}</p>
                <p>{frontmatter.childmale}</p>
                <p class={`text-lg font-bold ${frontmatter.txsc}`}>{frontmatter.parentmale}</p>
                <p>{frontmatter.citymale}</p>
                <div class="mt-4">
                  <a href={`https://www.instagram.com/${frontmatter.instagrammale}`} class="flex flex-row gap-2 items-center">
                    <Icon name="mdi:instagram" class={`w-10 h-10 p-2 rounded-full ${frontmatter.txfs} ${frontmatter.bgsc}`} />
                  </a>
                </div>
              </div>
            </div>
          </div>
          </div>
        </div>
      </div>
    </section>
    <section class=`animasi7 overflow-hidden relative ${frontmatter.bgth}`>
      <div class="container mx-auto py-20 px-6 relative">
        <div class=`text-center py-20 px-2 my-10 rounded-t-full max-w-4xl w-full mx-auto ${frontmatter.txfs} border-2 ${frontmatter.bdr}`>
