                    Google Maps
                    <Icon name="mdi:google-maps" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
                  </button>
                </a>
              </div>
            </div>
          </div>
          </div>
          <iframe src=`${frontmatter.linkmaps}` style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade" class=`fade-up opacity-0 mx-auto mt-12 w-full max-w-2xl h-56 md:h-96 rounded-lg p-1 ${frontmatter.bgth}`></iframe>
        </div>
      </div>
    </section>
    <section class="animasi8 overflow-hidden">
      <div class="container mx-auto px-6 py-20">
        <div class="text-center my-5 mb-10">
          <h2 class=`fade-down opacity-0 text-center font-montecarlo text-5xl lg:text-8xl ${frontmatter.txfs}`>
            Moment Kami
          </h2>
        </div>
        <div class="grid grid-cols-2 lg:grid-cols-4 justify-center gap-2 pb-20 [&>*]:w-full [&>*]:md:max-w-4xl [&>*]:p-1 [&>*]:relative">
          <div class="fade-right">
            <div class="truncate">
              <img
                src={frontmatter.imgstory1}
                alt="Foto 1"
                class="object-cover aspect-video transition-all duration-700	ease-linear hover:scale-125 hover:duration-700 hover:transition-all hover:ease-linear"
              />
            </div>
          </div>
          <div class="fade-down opacity-0">
            <div class="truncate">
              <img
                src={frontmatter.imgstory2}
                alt="Foto 1"
                class="object-cover aspect-video transition-all duration-700	ease-linear hover:scale-125 hover:duration-700 hover:transition-all hover:ease-linear"
              />
            </div>
          </div>
          <div class="fade-up opacity-0">
            <div class="truncate">
              <img
                src={frontmatter.imgstory3}
                alt="Foto 1"
                class="object-cover aspect-video transition-all duration-700	ease-linear hover:scale-125 hover:duration-700 hover:transition-all hover:ease-linear"
              />
            </div>
          </div>
          <div class="fade-left opacity-0">
            <div class="truncate">
              <img
                src={frontmatter.imgstory4}
                alt="Foto 1"
                class="object-cover aspect-video transition-all duration-700	ease-linear hover:scale-125 hover:duration-700 hover:transition-all hover:ease-linear"
              />
            </div>
          </div>
          <div class="fade-left opacity-0">
            <div class="truncate">
              <img
                src={frontmatter.imgstory5}
                alt="Foto 1"
                class="object-cover aspect-video transition-all duration-700	ease-linear hover:scale-125 hover:duration-700 hover:transition-all hover:ease-linear"
              />
            </div>
          </div>
          <div class="fade-up opacity-0">
            <div class="truncate">
              <img
                src={frontmatter.imgstory6}
                alt="Foto 1"
                class="object-cover aspect-video transition-all duration-700	ease-linear hover:scale-125 hover:duration-700 hover:transition-all hover:ease-linear"
              />
