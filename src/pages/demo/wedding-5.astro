            <div class=`fade-up opacity-0 font-josefin space-y-3 ${frontmatter.txfs}`>
              <p class="text-xl font-medium border-b-2 border-b-yellow-800">{ frontmatter.fullmale }</p>
              <p class="text-lg">{frontmatter.childmale}</p>
              <p class=`text-lg font-medium`>
                { frontmatter.parentmale }</p>
              <p>{frontmatter.citymale}</p>
              <div class="flex gap-3 text-center justify-center mt-4">
                <a href={`https://www.instagram.com/${frontmatter.instagrammale}`} class="flex flex-row gap-2 items-center">
                  <Icon name="mdi:instagram" class=`w-10 h-10 p-2 rounded-full ${frontmatter.txsc} ${frontmatter.bgfs}` />
                </a>
              </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <section class=`animasi6 relative bg-cover bg-no-repeat bg-center bg-fixed overflow-hidden before:content-[''] before:bg-gradient-to-b ${frontmatter.from} before:to-transparent before:inset-0 before:bg-cover before:absolute`
