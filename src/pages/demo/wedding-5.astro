            <h2 class="text-center font-cormorant text-4xl">Akad Nikah</h2>
            <p class="text-lg font-bold flex gap-2">
              <Icon name="mdi:date-range" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
              {frontmatter.datefs}
            </p>
            <p class="text-md flex gap-2">
              <Icon name="mdi:clock-time-eight-outline" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
              {frontmatter.timefs}
            </p>
            <p class="flex gap-2">
              <Icon name="mdi:home" class="w-6 h-6 flex-shrink-0 min-w-max" />
              {frontmatter.locationfs}
            </p>
            <p class="flex gap-2 text-left">
              <Icon name="mdi:google-maps" class="w-6 h-6 flex-shrink-0 min-w-max" />
              {frontmatter.addressfs}
            </p>
            <div class="group">
              <a href={frontmatter.mapsfs}>
                <button
                  class={`mx-auto mt-8 flex items-center justify-center rounded-full px-4 py-2 text-center text-sm font-medium ${frontmatter.txfs} ${frontmatter.bgsc}`}
                >
                  Google Maps
                  <Icon name="mdi:google-maps" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
                </button>
              </a>
            </div>
          </div>
        </div>
        <div class={`fade-left opacity-0 ${frontmatter.bgfs} ${frontmatter.txth}`}>
          <div class="font-poppins space-y-3">
            <h2 class="text-center font-cormorant text-4xl">Resepsi</h2>
            <p class="text-lg font-bold flex gap-2">
              <Icon name="mdi:date-range" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
              {frontmatter.datesc}
            </p>
            <p class="text-md flex gap-2">
              <Icon name="mdi:clock-time-eight-outline" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
