        <div class="bottom-0 right-0">
          <img src=`${frontmatter.dekorsc}` class="rotate-180" alt="JejakBahagia" />
        </div>
      </div>
      <div class="container mx-auto py-20 px-6 relative z-50">
        <div class=`text-center my-5 mb-10 ${frontmatter.txsc}`>
          <h2 class="fade-right opacity-0 text-2xl font-bold mt-5 md:mt-0 mb-3 font-cormorant">Where & When</h2>
          <h1 class="fade-left opacity-0 text-center font-cormorant text-5xl lg:text-8xl">
            Wedding Day
          </h1>
        </div>
        <div
          class="grid gird-cols-1 md:grid-cols-2 max-w-4xl w-full mx-auto justify-center items-center gap-4 mt-5 *:mx-auto *:rounded-2xl *:h-full *:px-6 *:md:px-12 *:py-8 *:w-full"
        >
        <div class={`fade-right opacity-0 ${frontmatter.bgth} ${frontmatter.txfs}`}>
          <div class="font-poppins space-y-3">
            <h2 class="text-center font-cormorant text-4xl">Akad Nikah</h2>
            <p class="text-lg font-bold flex gap-2">
              <Icon name="mdi:date-range" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
              {frontmatter.datefs}
            </p>
            <p class="text-md flex gap-2">
              <Icon name="mdi:clock-time-eight-outline" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
              {frontmatter.timefs}
            </p>
            <p class="flex gap-2">
              <Icon name="mdi:home" class="w-6 h-6 flex-shrink-0 min-w-max" />
              {frontmatter.locationfs}
            </p>
            <p class="flex gap-2 text-left">
              <Icon name="mdi:google-maps" class="w-6 h-6 flex-shrink-0 min-w-max" />
              {frontmatter.addressfs}
            </p>
            <div class="group">
              <a href={frontmatter.mapsfs}>
                <button
                  class={`mx-auto mt-8 flex items-center justify-center rounded-full px-4 py-2 text-center text-sm font-medium ${frontmatter.txfs} ${frontmatter.bgsc}`}
                >
                  Google Maps
                  <Icon name="mdi:google-maps" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
                </button>
              </a>
            </div>
          </div>
        </div>
        <div class={`fade-left opacity-0 ${frontmatter.bgth} ${frontmatter.txfs}`}>
          <div class="font-poppins space-y-3">
            <h2 class="text-center font-cormorant text-4xl">Resepsi</h2>
            <p class="text-lg font-bold flex gap-2">
              <Icon name="mdi:date-range" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
              {frontmatter.datesc}
            </p>
            <p class="text-md flex gap-2">
              <Icon name="mdi:clock-time-eight-outline" class="w-6 h-6 flex-shrink-0 min-w-[1.5rem]" />
              {frontmatter.timesc}
            </p>
            <p class="flex gap-2">
              <Icon name="mdi:home" class="w-6 h-6 flex-shrink-0 min-w-max" />
              {frontmatter.locationsc}
            </p>
            <p class="flex gap-2 text-left">
              <Icon name="mdi:google-maps" class="w-6 h-6 flex-shrink-0 min-w-max" />
              {frontmatter.addresssc}
            </p>
