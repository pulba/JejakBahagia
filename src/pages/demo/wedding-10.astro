        </div>
      </div>
    </section>
    <section  class=`animasi2 relative bg-cover bg-center bg-fixed mx-auto overflow-hidden before:content-[''] before:bg-gradient-to-b before:to-transparent before:inset-0 before:bg-cover before:absolute ${frontmatter.from} -mt-20 ${frontmatter.bgfs}` style={`background-image:url(${frontmatter.add1})`}>  
